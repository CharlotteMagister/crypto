<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.jcryptool.analysis"
            class="org.jcryptool.analysis.substitution.views.SubstitutionAnalysisView"
            icon="icons/view.gif"
            id="org.jcryptool.analysis.substitution.views.SubstitutionAnalysisView"
            name="%view.name">
      </view>
   </extension>
   <extension
         point="org.jcryptool.core.operations.analysis">
      <analysis
            name="%analysis.name2"
            viewId="org.jcryptool.analysis.substitution.views.SubstitutionAnalysisView">
      </analysis>
   </extension>
   
      <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="$nl$/contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="$nl$/toc.xml"
            primary="false">
      </toc>
   </extension>

</plugin>
